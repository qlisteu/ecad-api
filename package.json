{
  "name": "ecad-api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:clean": "npm run kill-port-ps && npm run dev",
    "kill-port": "for /f \"tokens=5\" %%a in ('netstat -ano ^| findstr :4000 ^| findstr LISTENING') do taskkill /f /pid %%a 2>nul",
    "kill-port-ps": "powershell -ExecutionPolicy Bypass -File kill-port.ps1",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "openai": "^4.0.0",
    "pdf-parse": "^1.1.1",
    "@supabase/supabase-js": "^2.45.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.18.8",
    "@types/pdf-parse": "^1.1.5",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vercel/node": "^5.6.5",
    "eslint": "^9.9.0",
    "jest": "^29.0.0",
    "nodemon": "^3.1.9",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
